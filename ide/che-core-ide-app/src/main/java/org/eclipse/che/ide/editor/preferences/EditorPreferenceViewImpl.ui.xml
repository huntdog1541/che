<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .scrollPanel {
            overflow-x: hidden !important;
        }
    </ui:style>
    <g:ScrollPanel width="100%" height="100%" styleName="{style.scrollPanel}">
        <g:HTMLPanel height="100%">
            <g:FlowPanel width="100%" height="100%" ui:field="editorPreferencesContainer" debugId="editorPreferencesContainer"/>
        </g:HTMLPanel>
    </g:ScrollPanel>
</ui:UiBinder>
